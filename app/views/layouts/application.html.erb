<!DOCTYPE html>
<html>
  <head>
    <title>Phinterface</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.10/css/all.css"> -->
   <link href="https://fonts.googleapis.com/css?family=Comfortaa:200,300,400,500,600,700|Nunito:200,300,300i,700&display=swap" rel="stylesheet">
    <%= favicon_link_tag asset_path('phi-logo-icon.png') %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- social media -->
    <meta property="og:title" content="Phi">
    <meta property="og:description" content="Prenez le contrôle de votre santé.">
    <meta property="og:image" content="https://res.cloudinary.com/nbprojects/image/upload/v1572256970/Phi/l0iti66qalkks7iqxi5c.jpg">
    <meta property="og:url" content="https://www.phinterface.com">
    <script src="https://kit.fontawesome.com/464e539501.js"></script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
